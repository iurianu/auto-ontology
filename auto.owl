<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY dbpedia "http://dbpedia.org/resource/" >
    <!ENTITY wiki "http://en.wikipedia.org/wiki/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY gr "http://purl.org/goodrelations/v1#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY auto "http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#" >
]>


<rdf:RDF xmlns="http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#"
     xml:base="http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:dbpedia="http://dbpedia.org/resource/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:auto="http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#"
     xmlns:gr="http://purl.org/goodrelations/v1#"
     xmlns:wiki="http://en.wikipedia.org/wiki/"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://iurianu.rocks/wp-content/uploads/cdn/auto.owl">
        <owl:versionInfo>http://iurianu.rocks/wp-content/uploads/cdn/auto.owl</owl:versionInfo>
        <creator>Iulian Andriescu</creator>
        <author>Iulian Andriescu</author>
        <description xml:lang="en">
            The Auto Ontology combines information from more related ontologies in order to create a common language to be used within the AUTO Industry.
        </description>
        <title xml:lang="en">The Automotive Industry Ontology</title>
        <rdfs:isDefinedBy rdf:resource="http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#gearboxTypeId -->

    <owl:AnnotationProperty rdf:about="&auto;gearboxTypeId">
        <rdfs:label xml:lang="en">gearboxTypeId (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The Volkswagen-specific gearbox type ID</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;TransmissionTypeValue"/>
    </owl:AnnotationProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#hasID -->

    <owl:AnnotationProperty rdf:about="&auto;hasID">
        <rdfs:label xml:lang="en">hasID (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">An annotation property that can be used to attach element identifiers from XML schemas for the corresponding XML data or schema.</rdfs:comment>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:AnnotationProperty rdf:about="&dc;subject"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="&dc;title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&dcterms;license"/>
    


    <!-- http://purl.org/goodrelations/v1#hasMaxValueFloat -->

    <owl:AnnotationProperty rdf:about="&gr;hasMaxValueFloat"/>
    


    <!-- http://purl.org/goodrelations/v1#hasMinValueFloat -->

    <owl:AnnotationProperty rdf:about="&gr;hasMinValueFloat"/>
    


    <!-- http://purl.org/goodrelations/v1#hasUnitOfMeasurement -->

    <owl:AnnotationProperty rdf:about="&gr;hasUnitOfMeasurement"/>
    


    <!-- http://purl.org/goodrelations/v1#hasValueFloat -->

    <owl:AnnotationProperty rdf:about="&gr;hasValueFloat"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#acceleration -->

    <owl:ObjectProperty rdf:about="&auto;acceleration">
        <rdfs:label xml:lang="en">Acceleration</rdfs:label>
        <rdfs:comment xml:lang="en">The time needed to accelerate the vehicle from a given start velocity to a given target velocity
Typical unit code(s): SEC for seconds

Note: There are unfortunately no standard unit codes for seconds/0..100 km/h or seconds/0..60 mph. Simply use &quot;SEC&quot; for seconds and indicate the velocities in the rdfs:label of the gr:QuantitativeValueFloat, or use auto:referenceSpeeds with auto:0to60MPH (0..60 mph) or auto:0to100KMH (0..100 km/h) to specify the reference speeds.</rdfs:comment>
        <rdfs:range rdf:resource="&gr;QuantitativeValueFloat"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;Boat"/>
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
                    <rdf:Description rdf:about="&auto;Ship"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#advancedPayment -->

    <owl:ObjectProperty rdf:about="&auto;advancedPayment">
        <rdfs:label xml:lang="en">Advanced Payment</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the advanced payment for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#availableChoice -->

    <owl:ObjectProperty rdf:about="&auto;availableChoice">
        <rdfs:label xml:lang="en">Available Choice</rdfs:label>
        <rdfs:comment xml:lang="en">This property links available Choices or Components to a Spec Item.</rdfs:comment>
        <rdfs:range rdf:resource="&auto;ChoiceOrComponent"/>
        <rdfs:domain rdf:resource="&auto;SpecItemCollection"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#axles -->

    <owl:ObjectProperty rdf:about="&auto;axles">
        <rdfs:label xml:lang="en">Axles</rdfs:label>
        <rdfs:comment xml:lang="en">The number of axles 
Typical unit code(s): C62</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;MotorizedRoadVehicle"/>
        <rdfs:range rdf:resource="&gr;QuantitativeValueInteger"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#bodyStyle -->

    <owl:ObjectProperty rdf:about="&auto;bodyStyle">
        <rdfs:label xml:lang="en">Body Style</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the design and body style of the vehicle. 

Use DBPedia resources to indicate the features, if possible. Popular identifiers are
  http://dbpedia.org/resource/Convertible
  http://dbpedia.org/resource/Hatchback
  http://dbpedia.org/resource/Station_wagon
  http://dbpedia.org/resource/Sport_utility_vehicle
  http://dbpedia.org/resource/Roadster

See also http://en.wikipedia.org/wiki/Car_body_styles.

If you cannot find a suitable identifier in DBPedia, define your own as an instance of auto:BodyStyleValue.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&wiki;Car_body_styles"/>
        <rdfs:range rdf:resource="&auto;BodyStyleValue"/>
        <rdfs:domain rdf:resource="&gr;ProductOrService"/>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#cargoVolume -->

    <owl:ObjectProperty rdf:about="&auto;cargoVolume">
        <rdfs:label xml:lang="en">cargoVolume (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The available volume for cargo or luggage. For automobiles, this is usually the trunk space.
Typical unit code(s): LTR for liters, FTQ for cubic foot/feet
    
Note: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.</rdfs:comment>
        <rdfs:range rdf:resource="&gr;QuantitativeValueFloat"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
                    <rdf:Description rdf:about="&auto;Watercraft"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#compatibility -->

    <owl:ObjectProperty rdf:about="&auto;compatibility">
        <rdfs:label xml:lang="en">Compatibility</rdfs:label>
        <rdfs:comment xml:lang="en">Links from a Derivative to information on whether a particular configuration is valid (true) or invalid (false)</rdfs:comment>
        <rdfs:range rdf:resource="&auto;ConfigurationInfo"/>
        <rdfs:domain rdf:resource="&auto;Derivative"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#compatibleWith -->

    <owl:ObjectProperty rdf:about="&auto;compatibleWith">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en">Compatible With</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates that two Choices or Components are compatible with each other.

Note: This property is symmetric, i.e., if A is compatible with B, then B is also compatible with A.</rdfs:comment>
        <rdfs:range rdf:resource="&auto;ChoiceOrComponent"/>
        <rdfs:domain rdf:resource="&auto;ChoiceOrComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#credit -->

    <owl:ObjectProperty rdf:about="&auto;credit">
        <rdfs:label xml:lang="en">Credit</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the credit for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#customerCashPrice -->

    <owl:ObjectProperty rdf:about="&auto;customerCashPrice">
        <rdfs:label xml:lang="en">Customer Cash Price</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the customer cash price for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#customerDeposit -->

    <owl:ObjectProperty rdf:about="&auto;customerDeposit">
        <rdfs:label xml:lang="en">Customer Deposit</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the customer deposit for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#default -->

    <owl:ObjectProperty rdf:about="&auto;default">
        <rdfs:label xml:lang="en">Default</rdfs:label>
        <rdfs:comment xml:lang="en">This property links the default Choice or Component to a Spec Item. 
Since it is defined a sub-property of coo:availableChoice, the default choice is automatically part of the available choices.</rdfs:comment>
        <rdfs:range rdf:resource="&auto;ChoiceOrComponent"/>
        <rdfs:domain rdf:resource="&auto;SpecItemCollection"/>
        <rdfs:subPropertyOf rdf:resource="&auto;availableChoice"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#dependsOn -->

    <owl:ObjectProperty rdf:about="&auto;dependsOn">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <rdfs:label xml:lang="en">Depends On</rdfs:label>
        <rdfs:comment xml:lang="en">Links a Choice or Component to one or multiple other Choices Or Components required for that choice  

Note: This property is transitive, i.e., if B is required by A and C and D are required by B, then C and D are also required by A.</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;ChoiceOrComponent"/>
        <rdfs:range rdf:resource="&auto;ChoiceOrComponent"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#depositContribution -->

    <owl:ObjectProperty rdf:about="&auto;depositContribution">
        <rdfs:label xml:lang="en">Deposit Contribution</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the deposit contribution for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#discount -->

    <owl:ObjectProperty rdf:about="&auto;discount">
        <rdfs:label xml:lang="en">Discount</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the discount for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#doors -->

    <owl:ObjectProperty rdf:about="&auto;doors">
        <rdfs:label xml:lang="en">doors (0..1)</rdfs:label>
        <rdfs:comment xml:lang="en">The number of doors
Typical unit code(s): C62</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;MotorizedRoadVehicle"/>
        <rdfs:range rdf:resource="&gr;QuantitativeValueInteger"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#driveWheelConfiguration -->

    <owl:ObjectProperty rdf:about="&auto;driveWheelConfiguration">
        <rdfs:label xml:lang="en">Drive Wheel Configuration</rdfs:label>
        <rdfs:comment xml:lang="en">The drive wheel configuration, i.e. which roadwheels will receive torque from the vehicle&apos;s engine via the drivetrain</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&wiki;Drive_wheel"/>
        <rdfs:range rdf:resource="&auto;DriveWheelConfigurationValue"/>
        <rdfs:domain rdf:resource="&auto;MotorizedRoadVehicle"/>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#engineDisplacement -->

    <owl:ObjectProperty rdf:about="&auto;engineDisplacement">
        <rdfs:label xml:lang="en">engineDisplacement (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">The volume swept by all of the pistons inside the cylinders of an internal combustion engine in a single movement. If there are multiple engines, simply attach this property multiple times.
Typical unit code(s): CMQ for cubic centimeter, LTR for liters, INQ for cubic inches
    
Note 1: You can link to information about how the given value has been determined using the gr:valueReference property.
Note 2: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&wiki;Engine_displacement"/>
        <rdfs:range rdf:resource="&gr;QuantitativeValueFloat"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;Boat"/>
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
                    <rdf:Description rdf:about="&auto;Ship"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#enginePower -->

    <owl:ObjectProperty rdf:about="&auto;enginePower">
        <rdfs:label xml:lang="en">enginePower (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">The power of the vehicle&apos;s engine. If there are multiple engines, simply attach this property multiple times.
Typical unit code(s): KWT for kilowatt 
    
Note 1: There are many different ways of measuring an engine&apos;s power. For an overview, see  http://en.wikipedia.org/wiki/Horsepower#Engine_power_test_codes.
Note 2: You can link to information about how the given value has been determined using the gr:valueReference property.
Note 3: You can use gr:hasMinValueFloat and gr:hasMaxValueFloat to indicate ranges.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&wiki;Horsepower"/>
        <rdfs:range rdf:resource="&gr;QuantitativeValueFloat"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;Boat"/>
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
                    <rdf:Description rdf:about="&auto;Ship"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#engineType -->

    <owl:ObjectProperty rdf:about="&auto;engineType">
        <rdfs:label xml:lang="en">Engine Type</rdfs:label>
        <rdfs:comment xml:lang="en">The type of engine or engines powering the vehicle. If there are multiple engines, simply attach this property multiple times.

Use DBPedia resources to indicate the engine type. Popular identifiers are
  http://dbpedia.org/resource/Two-stroke_engine
  http://dbpedia.org/resource/Four-stroke_engine
  http://dbpedia.org/resource/Wankel_engine
  http://dbpedia.org/resource/Electric_motor

If you cannot find a suitable identifier in DBPedia, define your own as an instance of auto:EngineTypeValue.</rdfs:comment>
        <rdfs:range rdf:resource="&auto;EngineTypeValue"/>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;Boat"/>
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
                    <rdf:Description rdf:about="&auto;Ship"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#excessMileage -->

    <owl:ObjectProperty rdf:about="&auto;excessMileage">
        <rdfs:label xml:lang="en">Excess Mileage</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the rate for excess mileage</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#feature -->

    <owl:ObjectProperty rdf:about="&auto;feature">
        <rdfs:label xml:lang="en">Feature</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates a built-in feature, accessory, or component of the vehicle.

Use DBPedia resources to indicate the features, if possible. Popular identifiers are
  http://dbpedia.org/resource/Anti-lock_braking_system
  http://dbpedia.org/resource/Electronic_stability_control
  http://dbpedia.org/resource/Armrest
  http://dbpedia.org/resource/Spoiler_%28automotive%29
  http://dbpedia.org/resource/CD_Changer
  http://dbpedia.org/resource/Cup_holder
  http://dbpedia.org/resource/Tow_hitch
  http://dbpedia.org/resource/Power_window
  http://dbpedia.org/resource/Power_steering
  http://dbpedia.org/resource/Air_conditioner
  http://dbpedia.org/resource/Child_safety_lock
  http://dbpedia.org/resource/Parking_sensors

See also http://en.wikipedia.org/wiki/Category:Automotive_accessories.

If you cannot find a suitable identifier in DBPedia, define your own as an instance of auto:FeatureValue.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&dbpedia;Category:Automotive_accessories"/>
        <rdfs:seeAlso rdf:resource="&wiki;Category:Automotive_accessories"/>
        <rdfs:range rdf:resource="&auto;FeatureValue"/>
        <rdfs:domain rdf:resource="&gr;ProductOrService"/>
        <rdfs:subPropertyOf rdf:resource="&gr;qualitativeProductOrServiceProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#featureOverride -->

    <owl:ObjectProperty rdf:about="&auto;featureOverride">
        <rdfs:label xml:lang="en">Feature Override</rdfs:label>
        <rdfs:comment xml:lang="en">Links a coo:ConfigurationInfo node to information on how a choice or component or the combination of multiple choices will override the regular properties of the resulting vehicle</rdfs:comment>
        <rdfs:range rdf:resource="&auto;PropertySpecification"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;ChoiceOrComponent"/>
                    <rdf:Description rdf:about="&auto;ConfigurationInfo"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#finalPaymentFee -->

    <owl:ObjectProperty rdf:about="&auto;finalPaymentFee">
        <rdfs:label xml:lang="en">Final Payment Fee</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the final payment fee for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#firstMonthlyPaymentFee -->

    <owl:ObjectProperty rdf:about="&auto;firstMonthlyPaymentFee">
        <rdfs:label xml:lang="en">First Monthly Payment Fee</rdfs:label>
        <rdfs:comment xml:lang="en">Indicates the first monthly payment fee for the financing</rdfs:comment>
        <rdfs:domain rdf:resource="&auto;Financing"/>
        <rdfs:range rdf:resource="&gr;PriceSpecification"/>
    </owl:ObjectProperty>
    


    <!-- http://iurianu.rocks/wp-content/uploads/cdn/auto.owl/#fuelConsumption -->

    <owl:ObjectProperty rdf:about="&auto;fuelConsumption">
        <rdfs:label xml:lang="en">fuelConsumption (0..*)</rdfs:label>
        <rdfs:comment xml:lang="en">The amount of fuel consumed for traveling a particular distance or temporal duration with the given vehicle (e.g. liters per 100 km)
Typical unit code(s): LTR for liters, GLL of US Gallons, GLI for UK / Imperial Gallons
        
Note 1: There are unfortunately no standard unit codes for &quot;liters per 100 km&quot;. Simply use &quot;LTR&quot; for liters, &quot;GLL&quot; of US Gallons, or &quot;GLI&quot; for UK / Imperial Gallons, and indicate the distance in the rdfs:label of the gr:QuantitativeValueFloat or use auto:referenceDistance to model the reference distance (e.g. 100 km). 
Note 2: There are two ways of indicating the fuel consumption, auto:fuelConsumption (e.g. 8 liters per 100 km) and auto:fuelEconomy (e.g. 30 miles per gallon). They are reciprocal.
Note 3: Often, the absolute value is useful only when related to driving speed (&quot;at 80 km/h&quot;) or usage pattern (&quot;city traffic&quot;). You can use gr:valueReference to link the value for the fuel consumption to another value.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="&wiki;Fuel_economy_in_automobiles"/>
        <rdfs:range rdf:resource="&gr;QuantitativeValueFloat"/>
        <rdfs:subPropertyOf rdf:resource="&gr;quantitativeProductOrServiceProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&auto;Boat"/>
                    <rdf:Description rdf:about="&auto;MotorizedRoadVehicle"/>
